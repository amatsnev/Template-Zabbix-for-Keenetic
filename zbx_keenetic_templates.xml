          delay: 1h
          item_prototypes:
            -
              uuid: 2c7860fab8db4d0eae4303e02ba1eb56
              name: 'Свободное место на диске {#SNMPINDEX} {#DSKPATH}'
              type: SNMP_AGENT
              snmp_oid: '.1.3.6.1.4.1.2021.9.1.7.{#SNMPINDEX}'
              key: 'dskAvail.[{#SNMPINDEX}]'
              delay: 1h
              trends: 90d
              units: B
