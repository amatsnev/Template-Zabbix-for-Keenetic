          type: SNMP_AGENT
          snmp_oid: 'discovery[{#ITEMVALUE}, .1.3.6.1.4.1.2021.9.1.1, {#DSKPATH}, .1.3.6.1.4.1.2021.9.1.2, {#DSKTOTAL}, .1.3.6.1.4.1.2021.9.1.6, {#DSKAVAIL}, .1.3.6.1.4.1.2021.9.1.7, {#DSKUSED}, .1.3.6.1.4.1.2021.9.1.8]'
