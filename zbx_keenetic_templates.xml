          uuid: f8a7fa9df68d4defadbfabf54428c782
          name: 'Средняя нагрузна ядро CPU'
          type: SNMP_AGENT
          snmp_oid: 'discovery[{#SNMPVALUE}, .1.3.6.1.4.1.2021.10.1.3]'
          key: laIndex
          item_prototypes:
            -
              uuid: 0bbdc3a95a694ab794a0eb87edb93569
              name: 'Средняя нагрузка на ядро {#SNMPINDEX} - {#SNMPVALUE}'
              type: SNMP_AGENT
              snmp_oid: '.1.3.6.1.4.1.2021.10.1.3.{#SNMPINDEX}'
              key: 'laLoad.[{#SNMPINDEX}]'
